<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns="http://xml.amadeus.com/TTKTIR_09_1_1A" elementFormDefault="qualified" targetNamespace="http://xml.amadeus.com/TTKTIR_09_1_1A" xmlns:xs="http://www.w3.org/2001/XMLSchema">
